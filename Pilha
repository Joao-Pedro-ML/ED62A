#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>




#define N 100

typedef struct {
    int key;

}objeto;

typedef struct {
    objeto vetor[N];
    int topo;
}pilhaEst;

void iniciaPilha(pilhaEst *pilha) {
    pilha->topo = -1;

}

bool estaVazia(pilhaEst *pilha) {
    if (pilha->topo == -1) {
        return true;
    } else {
        return false;
    }
}

bool estaCheia(pilhaEst *pilha) {
    if (pilha->topo == N - 1) {
        return true;
    } else {
        return false;
    }
}

void push(objeto obj, pilhaEst *pilha) {

    if (pilha->topo == N - 1) {
        printf("Pilha cheia!");
    } else {
        pilha->vetor[pilha->topo].key = obj.key;
        pilha->topo++;
    }
}

void pop(pilhaEst *pilha) {

}

int size(pilhaEst *pilha) {

}

objeto top(pilhaEst *pilha) {

}

void print(pilhaEst *pilha) {

    printf("{");
    for (int i = 0; i < 3; i++) {
        printf(" %i ", pilha->vetor[i].key);
    }
    printf("}");
}

int main(int argc, char** argv) {

    pilhaEst pilha;
    objeto obj;


    iniciaPilha(&pilha);

    for (int i = 0; i < 3; i++) {
        obj.key = i;
        push(obj, &pilha);
    }

    print(&pilha);

    return (EXIT_SUCCESS);
}

